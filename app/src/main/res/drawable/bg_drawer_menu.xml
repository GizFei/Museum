<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid
                android:color="#F6FAFB" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#0648c4fa"
                android:centerColor="#00FFFFFF"
                android:endColor="#00FFFFFF"
                />
        </shape>
    </item>
    <item
        android:top="-6dp"
        android:bottom="-6dp"
        android:right="-6dp">
        <shape>
            <stroke
                android:width="@dimen/tiny"
                android:color="@color/colorPrimary" />
        </shape>
    </item>
</layer-list>